{
    "name": "salesforce360",
    "version": "1.0.0",
    "description": "A SalesForce 360 Electron application",
    "main": "./dist/main.js",
    "entry": "./src/index.html",
    "scripts": {
        "build": "rm -rf dist && tsc --outDir dist && parcel build src/index.html --target electron --public-url .  --log-level 3",
        "start": "rm -rf dist && tsc --outDir dist && parcel watch src/index.html --target electron --public-url . --log-level 3 & sleep 5 && electron ."
    },
    "repository": "https://github.com/electron/electron-quick-start",
    "keywords": [
        "Electron",
        "quick",
        "start",
        "tutorial",
        "demo"
    ],
    "author": "GitHub",
    "license": "CC0-1.0",
    "devDependencies": {
        "@babel/preset-env": "^7.1.0",
        "@types/node": "v8",
        "@types/react": "^16.4.14",
        "@types/react-dom": "^16.0.7",
        "@types/superagent": "^3.8.4",
        "babel-preset-react": "^6.24.1",
        "electron": "^3.0.0",
        "electron-builder": "^20.28.4",
        "node-sass": "^4.9.3",
        "parcel-bundler": "^1.9.7",
        "typescript": "^3.0.3"
    },
    "dependencies": {
        "bootstrap": "^4.1.3",
        "mobx": "^5.1.2",
        "mobx-react": "^5.2.8",
        "react": "^16.5.2",
        "react-dom": "^16.5.2",
        "superagent": "^4.0.0-beta.5"
    }
}
